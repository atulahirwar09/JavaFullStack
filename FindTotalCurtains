public class Main
{
    public static int findTotalCurtains(int size, int[] arr) {
        int sum = 0;
        for(int a : arr) {
            int val = a / 12;
            sum += val;
        }
        
        return sum;
    }
    
	public static void main(String[] args) {
	    int[] arr = {3,42,60,6,14};
	    System.out.print(findTotalCurtains(5,arr));
	}
}
